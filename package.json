{
  "name": "via-scientific",
  "version": "1.0.0",
  "scripts": {
    "tsc:w": "tsc -w",
    "lint": "eslint --fix --quiet --ext .js,.ts ./src/ && tsc",
    "build": "npm run generate-routes && npx tsc -p ./tsconfig.json",
    "build:clean": "rm -rf ./dist/* && yarn build",
    "test": "ENV=test mocha",
    "start": "node ./dist/index.js",
    "dev": "npx tsoa routes && npx ts-node-dev --rs --cls --respawn ./src/index.ts",
    "generate-routes": "npx tsoa spec-and-routes",
    "scripts:fill-mongodb-with-tsv": "node -r ts-node/register src/scripts/fill-mongodb-with-tsv.ts"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/express": "4.17.6",
    "@types/express-serve-static-core": "4.17.7",
    "@types/lodash": "4.14.149",
    "@types/mocha": "5.2.7",
    "@types/node": "^14.0.0",
    "@types/sinon": "7.5.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "chai": "4.2.0",
    "csvtojson": "^2.0.10",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^35.4.1",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.4",
    "mocha": "6.2.2",
    "mongodb-memory-server": "8.9.5",
    "nock": "10.0.6",
    "sinon": "7.5.0",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "bluebird": "3.7.2",
    "chalk": "2.4.2",
    "clone": "2.1.2",
    "commander": "2.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "json-stringify-safe": "^5.0.1",
    "lodash": "4.17.15",
    "mongoose": "^5.13.15",
    "redoc-express": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.5.0",
    "tsoa": "^3.14.1",
    "uuid": "^8.3.2",
    "winston": "3.2.1",
    "winston-transport": "4.3.0",
    "yarn": "^1.22.17"
  }
}
